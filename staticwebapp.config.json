{
    "routes": [
        {
            "route": "/*"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html",
        "exclude": ["/assets/*.{png,jpg,gif}", "/static/*"]
    },
    "globalHeaders": {
        "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    }
}
